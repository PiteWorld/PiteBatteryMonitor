<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout 
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@drawable/cortnetschart"
        android:orientation="vertical">
    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal">
        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                style="@style/size10"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"
                android:text="@string/Battery_station"/>
              <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                android:gravity="center_vertical|right"
                android:text="@string/Battery_type2"/>
              	
              <TextView 
                    android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                android:gravity="center_vertical|right"
                android:text="@string/total_voltage"/>
               
                
        </LinearLayout>
        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            style="@style/size10"
            android:orientation="vertical">
            <TextView 
                android:id="@+id/internal_name"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                 android:gravity="center_vertical|right"/>
              <TextView 
                android:id="@+id/internal_Battery_type"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                style="@style/size10"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"/>
              <TextView 
                android:id="@+id/internal_total_voltage"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                style="@style/size10"
                android:textColor="@color/heise"
                 android:gravity="center_vertical|right"/>
        </LinearLayout>
        
        
        
         <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"
                style="@style/size10"
                android:text="@string/Battery_group"/>
              <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"
                style="@style/size10"
                android:text="@string/Internal_standard"/>
              
               <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                android:gravity="center_vertical|right"
                android:text="@string/total_current"/>
        </LinearLayout>
        
        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView 
                android:id="@+id/internal_Battery_group"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                 android:gravity="center_vertical|right"/>
            
              <TextView 
                android:id="@+id/internal_Internal_standard"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                 android:gravity="center_vertical|right"/>
              
                <TextView 
                android:id="@+id/internal_total_current"
                android:layout_width="match_parent"
                style="@style/size10"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"/>
        </LinearLayout>
        
        
        
        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                android:gravity="center_vertical|right"
                android:text="@string/Battery_manufacturers"/>
              <TextView android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                style="@style/size10"
                android:gravity="center_vertical|right"
                android:text="@string/Battery_num"/>
              <TextView
		                style="@style/size10"
		                android:layout_width="match_parent"
		                android:layout_height="30dp"
		                android:layout_gravity="center_vertical"
		                android:gravity="center_vertical|right"
		                android:text="@string/Battery_time1"
		                android:textColor="@color/heise" />
        </LinearLayout>
        
        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
                <TextView 
                android:id="@+id/internal_manufacturers"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                style="@style/size10"
                android:textColor="@color/heise"
                 android:gravity="center"/>
            
              <TextView 
                android:id="@+id/internal_Battery_num"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                style="@style/size10"
                android:textColor="@color/heise"
                 android:gravity="center"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout 
        android:layout_height="30dp"
        android:layout_width="match_parent"
        android:orientation="horizontal">
    	<LinearLayout android:layout_height="match_parent"
    	    android:layout_width="match_parent"
    	    android:layout_weight="1"
    	    android:orientation="horizontal">
              <LinearLayout android:layout_height="match_parent"
                  android:layout_width="match_parent">
              <TextView android:layout_width="match_parent"
                  android:layout_weight="1"
                android:layout_height="30dp"
                android:textColor="@color/heise"
                android:gravity="center_vertical|right"
                style="@style/size10"
                android:text="@string/Battery_status"/>
             <TextView
                 android:layout_weight="1"
                android:id="@+id/internal_Battery_status"
                style="@style/size10"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:gravity="center_vertical|right"
                android:textColor="@color/heise" /> 
                </LinearLayout>
         </LinearLayout>
         <LinearLayout android:layout_height="match_parent"
             android:layout_width="match_parent"
             android:orientation="horizontal"
             android:layout_weight="1">
              <LinearLayout android:layout_height="match_parent"
                  android:layout_width="match_parent">
             <TextView 
                 android:layout_height="match_parent"
                 android:gravity="center_vertical|right"
                 android:layout_width="match_parent"
                 android:layout_weight="1"
                 android:textColor="@color/heise"
                 android:text="@string/pite_battery_charge"
                 style="@style/size10"/>
             <TextView android:id="@+id/internal_battery_charge"
                 android:layout_height="match_parent"
                 android:layout_width="match_parent"
                 android:gravity="center"
                 android:layout_weight="1"
                  android:textColor="@color/heise"
                 android:layout_marginLeft="3dp"
                 style="@style/size10"/>
              </LinearLayout>
         </LinearLayout>
         <LinearLayout android:layout_height="match_parent"
             android:layout_width="match_parent"
             android:layout_weight="1">
              <Spinner
                android:id="@+id/internal_sp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/cortnetschart"
                android:focusable="false"
                android:focusableInTouchMode="false"/>
         </LinearLayout>
    </LinearLayout>
    </LinearLayout>
        
    <LinearLayout android:layout_height="30dp"
        android:layout_width="match_parent">
        <LinearLayout android:layout_height="match_parent"
            android:layout_width="match_parent"
	            android:layout_weight="1">
			<TextView android:layout_height="30dp"
			    android:layout_width="wrap_content"
			    android:textColor="@color/heise"
			    android:layout_marginLeft="5dp"
			    android:gravity="center"
			    style="@style/size10"
			    android:text="@string/R_average"/>
			<TextView 
			    android:id="@+id/internal_R_average"
			    android:layout_height="match_parent"
			    android:layout_width="match_parent"
			     style="@style/size10"
			    android:textColor="@color/heise"
			    android:gravity="center"/>
		</LinearLayout>
		 <LinearLayout android:layout_height="match_parent"
            android:layout_width="match_parent"
	            android:layout_weight="1">
			<TextView android:layout_height="30dp"
			    android:layout_width="wrap_content"
			    android:textColor="@color/heise"
			    android:layout_marginLeft="5dp"
			    android:gravity="center"
			    style="@style/size10"
			    android:text="@string/R_mean"/>
			<TextView 
			    android:id="@+id/internal_R_mean"
			    android:layout_height="match_parent"
			     style="@style/size10"
			    android:textColor="@color/heise"
			    android:layout_width="match_parent"
			    android:gravity="center"/>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout 
	    android:id="@+id/internal_analysis_title"
	    android:layout_height="30dp"
	    android:layout_width="match_parent"
	    android:orientation="horizontal"
	    android:background="@drawable/title_cell">
	</LinearLayout>
    <ListView android:id="@+id/internal_analysis_lv"
        android:dividerHeight="1dp"
        android:background="@drawable/content_cell"
        android:divider="@drawable/line"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
    </ListView>
</LinearLayout>
