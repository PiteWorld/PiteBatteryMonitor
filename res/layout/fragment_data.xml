<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/cortnetschart"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_station"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_type2"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/total_voltage"
                    android:textColor="@color/heise" />
            </LinearLayout>

            <LinearLayout
                style="@style/size10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/Data_name"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />

                <TextView
                    android:id="@+id/Data_Battery_type"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />

                <TextView
                    android:id="@+id/Data_total_voltage"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_group"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Internal_standard"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/total_current"
                    android:textColor="@color/heise" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/Data_Battery_group"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />

                <TextView
                    android:id="@+id/Data_Internal_standard"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />

                <TextView
                    android:id="@+id/Data_total_current"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/heise" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_manufacturers"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_num"
                    android:textColor="@color/heise" />

                <TextView
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical|right"
                    android:text="@string/Battery_time1"
                    android:textColor="@color/heise" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/Data_manufacturers"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center"
                    android:textColor="@color/heise" />

                <TextView
                    android:id="@+id/Data_Battery_num"
                    style="@style/size10"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center"
                    android:textColor="@color/heise" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        style="@style/size10"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:text="@string/Battery_status"
                        android:textColor="@color/heise" />

                    <TextView
                        android:id="@+id/Data_Battery_status"
                        style="@style/size10"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textColor="@color/heise" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:id="@+id/His_sydl"
                        style="@style/size10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:text="@string/pite_battery_charge"
                        android:textColor="@color/heise" />

                    <TextView
                        android:id="@+id/data_harged_state"
                        style="@style/size10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="3dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="@color/heise" />
                </LinearLayout>
            </LinearLayout>
           
            
          <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >
            <!--  <Listview
                    android:id="@+id/list_testTime"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >
 -->
 
                    <Spinner
                        android:id="@+id/Data_Battery_time1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/cortnetschart"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:gravity="center" />
                <!-- </Listview> -->
</LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/data_SOCMax"
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="@string/R_max"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_R_max1"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_R_max2"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_R_max3"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="@string/R_max"
            android:textColor="@color/heise"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/Data_R_max4"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_R_max5"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_R_max6"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="@string/U_High"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_high1"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_high2"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_high3"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="@string/U_low"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_low1"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_low2"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />

        <TextView
            android:id="@+id/Data_U_low3"
            style="@style/size10"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/heise" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Data_conclusion"
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="@string/conclusion"
            android:textColor="@color/heise" />

        <TextView
            style="@style/size10"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:textColor="@color/heise" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/data_analysis_title"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@drawable/title_cell"
        android:orientation="horizontal" >
    </LinearLayout>

    <ListView
        android:id="@+id/data_analysis_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/content_cell"
        android:divider="@drawable/line"
        android:dividerHeight="1dp" >
    </ListView>

</LinearLayout>